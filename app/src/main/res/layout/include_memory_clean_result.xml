        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:gravity="center"
        android:background="#f5f5f5"
        android:layout_weight="5"
        android:visibility="gone">
        <include layout="@layout/include_memory_clean_ad" />
    </RelativeLayout>-->
</FrameLayout>